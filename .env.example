# Social Login Configuration
# Copy this file to .env and fill in your actual values

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here

# Apple Sign-In Configuration  
APPLE_CLIENT_ID=your_apple_client_id_here

# Line Login Configuration
LINE_CLIENT_ID=your_line_client_id_here

# Telegram Login Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_BOT_USERNAME=your_telegram_bot_username_here

# General Configuration
SOCIAL_REDIRECT_URI=http://localhost:3000/auth/callback
ENABLED_PLATFORMS=google,apple,line,telegram